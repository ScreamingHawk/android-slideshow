	<!-- Settings -->

	<string name="title_activity_settings">Inställningar</string>

	<string name="pref_title_show_hidden_files">Visa dolda filer</string>
	<string name="pref_description_show_hidden_files">Visa filer som är dolda av andra filhanterare.
	</string>

	<string name="pref_title_show_thumbnails">Visa minatyrer</string>
	<string name="pref_description_show_thumbnails">Visa minatyrbilder. Stäng av detta för att öka prestanda,
		observera att du inte kommer att kunna se vilka filer som är bilder och vilka som inte är det.
	</string>

	<string name="pref_title_play_from_here">Aktivera spela upp bildspel härifrån</string>
	<string name="pref_description_play_from_here">"Spela upp bildspel härifrån"-knappen visas i fillistan och låter dig
		börja ett bildspel från den mapp du står i och inkluderar alla undermappar.
		Funktionen är avaktiverad som default eftersom den kan ha negativ påverkan på prestandan.
	</string>

	<string name="pref_title_use_device_root">Använd enhetens rotkatalog</string>
	<string name="pref_description_use_device_root">Aktivera navigering till rotkatalogen för enheten. Detta gör att du får tillgång till filer överallt på enheten,
		även extern lagring så som SD-kort. Notis: Kan kräva rootad enhet för att fungera korrekt.
	</string>

	<string name="pref_title_slide_delay">Fördröjning</string>
	<string name="pref_description_slide_delay">Klicka för att ändra hur många sekunder varje bild visas.
	</string>

	<string name="pref_title_image_details">Visa bilddetaljer</string>
	<string name="pref_description_image_details">Visar detaljerad information om bilden under bildspelets gång.
	</string>

	<string name="pref_title_stop_on_complete">Stoppa efter sista bilden</string>
	<string name="pref_description_stop_on_complete">Stoppa bildspelet efter att alla bilder har visats en gång.
	</string>

	<string name="pref_title_reverse_order">Omvänd ordning</string>
	<string name="pref_description_reverse_order">Visa bildspelet i omvänd ordning.
	</string>

	<string name="pref_title_random_order">Slumpmässig ordning</string>
	<string name="pref_description_random_order">Visa bildspelet i slumpmässig ordning.
	</string>
